<?xml version="1.0" encoding="utf-8"?>
  <plot>
    <title>Windgeschwindigkeit (㎞/h)</title>
    <yrange>0, 30</yrange>
    <grid>ytics</grid>
    <source>hourly</source>
    <subplot>
      <colour>4</colour>
      <ycalc>wind_kmph(data['wind_gust'])</ycalc>
      <title>Boe</title>
    </subplot>
    <subplot>
      <colour>3</colour>
      <ycalc>wind_kmph(data['wind_ave'])</ycalc>
      <title>durchschn.</title>
    </subplot>
  </plot>
